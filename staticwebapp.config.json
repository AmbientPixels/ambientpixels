{
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": [
      "/images/*",
      "/*.css",
      "/*.js",
      "/*.json",
      "/*.svg",
      "/*.png",
      "/*.ico",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif"
    ]
  },
  "routes": [
    {
      "route": "/cardforge/loadcards",
      "methods": [
        "GET"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    },
    {
      "route": "/cardforge/savecards",
      "methods": [
        "POST"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    },
    {
      "route": "/cardforge/cards",
      "methods": [
        "GET",
        "OPTIONS"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    },
    {
      "route": "/cardforge/gallery",
      "methods": [
        "GET",
        "OPTIONS"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    },
    {
      "route": "/cardforge/mycards",
      "methods": [
        "GET"
      ],
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/cardforge/cardpublish/*",
      "methods": [
        "POST"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}