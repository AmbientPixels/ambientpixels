{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "deviantart-proxy",
      "maxConcurrentRequests": 100,
      "maxOutstandingRequests": 20
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host": "Error",
      "Function": "Error",
      "Host.Aggregator": "Information"
    }
  }
}
